/* 水平间距 */
swiper {
  height: 330rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.notice-container {
  background-color: #2b4b6b;
  height: 40px;
  padding: 0 10px;
}
.notice-container .notice-box {
  height: 36px;
  background-color: #ffffff;
  border-radius: 10px;
  padding: 0 5px;
  width: 100%;
  display: flex;
  align-items: center;
  white-space: nowrap;
  font-size: 15px;
}
.notice-container .notice-box .notice-swiper {
  height: 36px;
  width: 100%;
}
.notice-container .notice-box .notice-swiper .notice-item {
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 36px;
  font-size: 15px;
}
.gongxu-container {
  background-color: #2b4b6b;
  height: 160px;
  padding: 10px 10px;
}
.gongxu-container .gongxu-box {
  height: 150px;
  background-color: #ffffff;
  border-radius: 10px;
  padding: 0 5px;
}
.gongxu-container .gongxu-box .gongxu-list {
  display: flex;
}
.gongxu-container .gongxu-box .gongxu-list .gongxu-item {
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.gongxu-container .gongxu-box .gongxu-list .gongxu-item .gongxu-img {
  width: 75%;
}
.center-container {
  background-color: #2b4b6b;
  height: 160px;
  padding: 10px 10px;
}
.center-container .center-box {
  height: 150px;
  background-color: #ffffff;
  border-radius: 10px;
  padding: 0 5px;
}
.center-container .center-box .center-list {
  display: flex;
}
.center-container .center-box .center-list .center-item {
  width: 33.33%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.center-container .center-box .center-list .center-item .center-img {
  width: 60%;
}
.placeholder {
  font-size: 15px;
  margin-left: 5px;
  margin-right: 5px;
}

